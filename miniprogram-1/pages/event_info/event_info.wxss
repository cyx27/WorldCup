.big_match {
  display: flex;
  flex-direction: column;
  font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;
  background-color: gainsboro;
}
.flag {
  height: 50rpx;
  width: 50rpx;
  align-items:center; 
}
.title{
 font-weight: 1000;
 padding-top: 15rpx;
 padding-bottom: 15rpx;
 padding-left: 30rpx;
 padding-right: 70rpx;
 float: left;
 font-size: 30rpx;
 color: rgb(252, 250, 250);
 background-color: rgb(112, 112, 114);
 width: 100%;
 border-top-color: rgb(250, 249, 249);
 border-top-width: 7rpx;
 border-top-style: solid;
 border-bottom-color: rgb(253, 253, 253);
 border-bottom-width: 4rpx;
 border-bottom-style: solid;
} 
.content{
  display: flex;
  flex-direction: row;
  align-items: center;
  padding-top: 15rpx;
  padding-bottom: 15rpx;
  padding-left: 30rpx;
  padding-right: 70rpx;
  background-color: rgb(146, 145, 145);
  border-top:rgb(250, 246, 246);
  border-top-width: 1rpx;
  border-top-style: solid;
  color: white;
}
page {
  background-color: rgb(250, 249, 249);
}
.topbarfather{
  position: relative;
  background-color: #ffffff;
  width: 100%;
  height: 50rpx;
}
.topbar{
  position: absolute;
  width: 50%;
  height: 50rpx;
  font-size: 35rpx;
  text-align: center;
  color: gainsboro;
}
.scheduletit{
  width: 100%;
  height: 100rpx;
  background: #45b97c;
  color: #fff;
  text-align: center;
  font-size: 32rpx;
  position: relative;
}
.next{
  position: absolute;
  right: 40rpx;
  top: 25%;
}
.head{
  position: absolute;
  left: 40rpx;
  top: 25%;
}
.whitebar{
  width: 100%;
  position: relative;
  height: 90rpx;
  line-height: 90rpx;
  background: #fffefe;
  display: flex;
}
.greybar{
  width: 100%;
  position: relative;
  height: 90rpx;
  line-height: 90rpx;
  background: #f7f7f7;
  display: flex;
}
.time{
  position:absolute;
  font-size: 30rpx;
  left:5%;
  width: 40%;
}
.teamname{
  font-size: 28rpx;
  position:absolute;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  width: 120rpx;
}
.pick{
  color:black;
  border-bottom:3rpx solid black;
}